{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",

  "compilerOptions" : {
 "target": "ES2020",
 "module": "commonjs" ,
 "outDir": "./dist",
 "rootDir": "./src",
 "strict": true,
 "esModuleInterop" : true,
 "forceConsistentCasingInFileNames" : true,
 "skipLibCheck" : true,
 "moduleResolution" : "node"
 }

}
